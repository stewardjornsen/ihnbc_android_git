<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.ihnbc.app" version="1.0.1">
  <name>IHNBC Mobile</name>
  <description>An IHNBC Church application for updates and events.</description>
  <author href="http://ihnbc.com" email="support@stewardxpress.com">IHNBC Team</author>
  <content src="index.html"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="14"/>
  <preference name="orientation" value="auto" />
  <preference name="loadUrlTimeoutValue" value="700000" />
  <preference name="android-installLocation" value="auto" />
  <plugin name="cordova-plugin-battery-status" source="npm" spec="~1.1.1"/>
  <plugin name="cordova-plugin-camera" source="npm" spec="~2.1.1"/>
  <plugin name="cordova-plugin-media-capture" source="npm" spec="~1.2.0"/>
  <plugin name="cordova-plugin-console" source="npm" spec="~1.0.2"/>
  <plugin name="cordova-plugin-contacts" source="npm" spec="~2.0.1"/>
  <plugin name="cordova-plugin-device" source="npm" spec="~1.1.1"/>
  <plugin name="cordova-plugin-device-motion" source="npm" spec="~1.2.0"/>
  <plugin name="cordova-plugin-device-orientation" source="npm" spec="~1.0.2"/>
  <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.2.0"/>
  <plugin name="cordova-plugin-file" source="npm" spec="~4.1.1"/>
  <plugin name="cordova-plugin-file-transfer" source="npm" spec="~1.5.0"/>
  <plugin name="cordova-plugin-geolocation" source="npm" spec="~2.1.0"/>
  <plugin name="cordova-plugin-globalization" source="npm" spec="~1.0.3"/>
  <plugin name="cordova-plugin-inappbrowser" source="npm" spec="~1.3.0"/>
  <plugin name="cordova-plugin-media" source="npm" spec="~2.2.0"/>
  <plugin name="cordova-plugin-network-information" source="npm" spec="~1.2.0"/>
  <plugin name="cordova-plugin-splashscreen" source="npm" spec="~3.2.1"/>
  <plugin name="cordova-plugin-statusbar" source="npm" spec="~2.1.2"/>
  <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1"/>
  <icon src="res/icons/android/icon-144-xxhdpi.png"/>
  <icon src="icon.png" gap:role="default" />
   <splash src="splash.png" />
  <platform name="android">
  <preference name="android-manifest/@android:installLocation" value="auto" />
    <icon src="res/icons/android/icon-36-ldpi.png" density="ldpi"/>
    <icon src="res/icons/android/icon-48-mdpi.png" density="mdpi"/>
    <icon src="res/icons/android/icon-72-hdpi.png" density="hdpi"/>
    <icon src="res/icons/android/icon-96-xhdpi.png" density="xhdpi"/>
    <icon src="res/icons/android/icon-144-xxhdpi.png" density="xxhdpi"/>
    <icon src="res/icons/android/icon-192-xxxhdpi.png" density="xxxhdpi"/>
    <splash src="res/screen/android/screen-ldpi-portrait.png" density="land-ldpi"/>
    <splash src="res/screen/android/screen-mdpi-portrait.png" density="land-mdpi"/>
    <splash src="res/screen/android/screen-hdpi-portrait.png" density="land-hdpi"/>
    <splash src="res/screen/android/screen-xhdpi-portrait.png" density="land-xhdpi"/>
	
  </platform>
  <platform name="ios">
    <icon src="res/icons/ios/icon.png" platform="ios" width="57" height="57"/>
    <icon src="res/icons/ios/icon@2x.png" platform="ios" width="114" height="114"/>
    <icon src="res/icons/ios/icon-40.png" platform="ios" width="40" height="40"/>
    <icon src="res/icons/ios/icon-40@2x.png" platform="ios" width="80" height="80"/>
    <icon src="res/icons/ios/icon-50.png" platform="ios" width="50" height="50"/>
    <icon src="res/icons/ios/icon-50@2x.png" platform="ios" width="100" height="100"/>
    <icon src="res/icons/ios/icon-60.png" platform="ios" width="60" height="60"/>
    <icon src="res/icons/ios/icon-60@2x.png" platform="ios" width="120" height="120"/>
    <icon src="res/icons/ios/icon-60@3x.png" platform="ios" width="180" height="180"/>
    <icon src="res/icons/ios/icon-72.png" platform="ios" width="72" height="72"/>
    <icon src="res/icons/ios/icon-72@2x.png" platform="ios" width="144" height="144"/>
    <icon src="res/icons/ios/icon-76.png" platform="ios" width="76" height="76"/>
    <icon src="res/icons/ios/icon-76@2x.png" platform="ios" width="152" height="152"/>
    <icon src="res/icons/ios/icon-small.png" platform="ios" width="29" height="29"/>
    <icon src="res/icons/ios/icon-small@2x.png" platform="ios" width="58" height="58"/>
    <icon src="res/icons/ios/icon-small@3x.png" platform="ios" width="87" height="87"/>
    <splash src="res/screen/ios/Default-568h@2x~iphone.png" platform="ios" width="640" height="1136"/>
    <splash src="res/screen/ios/Default-667h.png" platform="ios" width="750" height="1334"/>
    <splash src="res/screen/ios/Default-736h.png" platform="ios" width="1242" height="2208"/>
    <splash src="res/screen/ios/Default-Landscape-736h.png" platform="ios" width="2208" height="1242"/>
    <splash src="res/screen/ios/Default-Landscape@2x~ipad.png" platform="ios" width="2048" height="1536"/>
    <splash src="res/screen/ios/Default-Landscape~ipad.png" platform="ios" width="1024" height="768"/>
    <splash src="res/screen/ios/Default-Portrait@2x~ipad.png" platform="ios" width="1536" height="2048"/>
    <splash src="res/screen/ios/Default-Portrait~ipad.png" platform="ios" width="768" height="1024"/>
    <splash src="res/screen/ios/Default@2x~iphone.png" platform="ios" width="640" height="960"/>
    <splash src="res/screen/ios/Default~iphone.png" platform="ios" width="320" height="480"/>
  </platform>
  <platform name="wp8">
    <icon src="res/icons/wp8/ApplicationIcon.png" platform="wp8" width="99" height="99"/>
    <icon src="res/icons/wp8/Background.png" platform="wp8" width="159" height="159"/>
    <splash src="res/screen/wp8/SplashScreenImage.jpg" platform="wp8" width="768" height="1280"/>
  </platform>
  <platform name="windows">
    <icon src="res/icons/windows/logo.png" platform="windows" width="150" height="150"/>
    <icon src="res/icons/windows/smalllogo.png" platform="windows" width="30" height="30"/>
    <icon src="res/icons/windows/storelogo.png" platform="windows" width="50" height="50"/>
    <splash src="res/screen/windows/splashscreen.png" platform="windows" width="620" height="300"/>
  </platform>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
</widget>